{
    "name": "Gate membership to label",
    "description": "This operator takes as input a table of gate memberships per event and outputs a label based on gate names.",
    "tags": ["gating", "labeling", "flow cytometry"],
    "authors": ["tercen"],
    "urls": ["https://github.com/tercen/gate_membership_to_label_operator"],
    "container":"ghcr.io/tercen/gate_membership_to_label_operator:0.0.4",
    "properties": [],
    "operatorSpec": {
        "kind": "OperatorSpec",
        "ontologyUri": "https://tercen.com/_ontology/tercen",
        "ontologyVersion": "0.0.1",
        "inputSpecs": [
        {
            "kind": "CrosstabSpec",
            "metaFactors": [
            {
                "kind": "MetaFactor",
                "name": "Event",
                "type": "",
                "description": "Unique event identifiers",
                "ontologyMapping": "event",
                "crosstabMapping": "row",
                "cardinality": "1..n",
                "factors": []
            },
            {
                "kind": "MetaFactor",
                "name": "Gate",
                "type": "",
                "description": "Gate name identifiers",
                "ontologyMapping": "gate",
                "crosstabMapping": "column",
                "cardinality": "1..n",
                "factors": []
            }
            ],
            "axis": [
            {
                "kind": "AxisSpec",
                "metaFactors": [
                {
                    "kind": "MetaFactor",
                    "name": "Gate Membership",
                    "type": "numeric",
                    "description": "Gate membership value (1 for positive, missing for negative)",
                    "ontologyMapping": "membership",
                    "crosstabMapping": "y",
                    "cardinality": "1",
                    "factors": []
                }
                ]
            }
            ]
        }
        ],
        "outputSpecs": []
  }
}
